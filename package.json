{
  "name": "nowliveweb",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "next build",
    "dev": "next dev",
    "export": "next build && next export",
    "fmt": "prettier . --write --ignore-path .gitignore",
    "fmt:check": "prettier . --check --ignore-path .gitignore",
    "lint": "next lint",
    "pre-commit": "bun run fmt:check && bun run lint",
    "prepare": "husky install",
    "start": "next start"
  },
  "dependencies": {
    "@tabler/icons-react": "^3.6.0",
    "next": "14.2.4",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "ua-parser-js": "^1.0.38"
  },
  "devDependencies": {
    "@types/node": "20.14.5",
    "@types/react": "18.3.3",
    "@types/ua-parser-js": "^0.7.39",
    "@typescript-eslint/eslint-plugin": "7.13.1",
    "@typescript-eslint/parser": "7.13.1",
    "eslint": "8.56",
    "eslint-config-next": "14.2.4",
    "eslint-config-prettier": "9.1.0",
    "husky": "9.0.11",
    "prettier": "3.3.2",
    "sass": "1.77.6",
    "typescript": "5.4.5"
  },
  "trustedDependencies": [
    "core-js-pure"
  ]
}
